{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public",
    "tag": "prerelease"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joaoviana/breakpoints.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "sc-breakpoints",
  "author": "Joao Viana",
  "module": "dist/sc-breakpoints.esm.js",
  "size-limit": [
    {
      "path": "dist/sc-breakpoints.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sc-breakpoints.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@size-limit/preset-small-lib": "^5.0.2",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "react": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-styled-components": "^1.12.0"
  },
  "dependencies": {
    "styled-components": "^5.3.0"
  }
}
